.nui-btn.flex-grow {
  flex-grow: 1;
}

.nui-btn--multiline .nui-btn-text {
  white-space: normal;
  text-overflow: clip;
  overflow: visible;
}

.nui-btn-bg {
  flex-direction: row;
  flex-grow: 1;
  padding-left: 4px;
  padding-right: 4px;
  border-radius: 4px;
  border-width: 1px;
  border-color: transparent;
}
.nui-btn-circle > .nui-btn-bg {
  border-radius: 50%;
}

.nui-btn-text {
  -unity-text-align: middle-left;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 12px;
  flex-grow: 1;
  padding-left: 4px;
  padding-right: 4px;
}

.nui-aspect-fitter {
  width: 100%;
  height: 0;
}

.nui-aspect-fitter__container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.nui-window {
  position: absolute;
}

.nui-window__container {
  border-radius: 16px;
  background-color: #1b1a1b;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  border-color: rgba(255, 255, 255, 0.0039215686);
  border-width: 1px;
}

.nui-window__title {
  -unity-text-align: middle-left;
  font-size: 14px;
  padding-left: 10px;
  padding-right: 10px;
}

.nui-window__background {
  position: absolute;
  top: -18px;
  bottom: -20px;
