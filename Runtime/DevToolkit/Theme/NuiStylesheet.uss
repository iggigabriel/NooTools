.nui-shadow {
  position: absolute;
  top: -18px;
  bottom: -20px;
  left: -18px;
  right: -18px;
  background-image: url(../Theme/Images/dropShadow64.png);
  -unity-slice-left: 31;
  -unity-slice-right: 31;
  -unity-slice-top: 31;
  -unity-slice-bottom: 31;
  -unity-background-image-tint-color: rgba(0, 0, 0, 0.5);
}

:root {
  font-size: 12px;
  color: #d8dad7;
  -unity-font-definition: url(../Theme/Fonts/NotoSans/NotoSans.ttf);
  flex-grow: 1;
}

TemplateContainer {
  flex-grow: 1;
  flex-direction: row;
}

.nui-row {
  flex-direction: row;
}
.nui-row.nui--reverse {
  flex-direction: row-reverse;
}

.nui-col {
  flex-direction: column;
}
.nui-col.nui--reverse {
  flex-direction: column-reverse;
}

:disabled {
  opacity: 0.4;
}
:disabled :disabled {
  opacity: 1;
}

.overflow-hidden {
  overflow: hidden;
}

.display-flex {
  display: flex;
}

.display-none {
  display: none;
}

.nui--transparent {
  opacity: 0;
}

.nui-mat-icon {
  -unity-font-style: normal;
  -unity-font-definition: url(../Theme/Fonts/MaterialIcons/MaterialIcons-Regular.ttf);
  -unity-text-align: middle-center;
  font-size: 18px;
  flex-shrink: 0;
}

.nui-btn {
